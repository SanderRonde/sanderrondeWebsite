{
	"name": "sanderrondewebsite",
	"version": "1.0.0",
	"description": "Sander Ronde Website",
	"main": "app/server/app.js",
	"nodemonConfig": {
		"ext": "js",
		"exec": "node app/server/app.js --dev --http 1234",
		"watch": [
			"app/server/"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SanderRonde/sanderrondewebsite.git"
	},
	"keywords": [
		"personal",
		"website",
		"portfolio",
		"sander",
		"ronde"
	],
	"author": "Sander Ronde <awsdfgvhbjn@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SanderRonde/sanderrondewebsite/issues"
	},
	"homepage": "https://github.com/SanderRonde/sanderrondewebsite#readme",
	"dependencies": {
		"express": "^4.14.1",
		"fs-extra": "^9.0.0",
		"lit-html": "^1.2.1",
		"path": "^0.12.7",
		"serve-static": "^1.13.2",
		"wc-lib": "^1.1.3"
	},
	"devDependencies": {
		"@types/express": "^4.16.1",
		"@types/fs-extra": "^8.1.0",
		"@types/node": "^11.11.3",
		"@types/serve-static": "^1.13.2",
		"nodemon": "^2.0.2",
		"ts-node": "^8.8.1",
		"typescript": "^3.6.2"
	},
	"scripts": {
		"compile:server": "tsc -w -p app/server/tsconfig.json",
		"compile:client": "tsc -w -p app/client/tsconfig.json",
		"watch:server": "npm run compile:server -- -w",
		"watch:client": "npm run compile:client -- -w"
	}
